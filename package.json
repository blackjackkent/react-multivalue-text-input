{
	"name": "react-multivalue-text-input",
	"version": "1.0.0",
	"description": " A text input component for React which maintains and displays a collection of entered values as an array of strings.",
	"browser": "build/index.js",
	"main": "build/index.js",
	"homepage": "https://blackjackkent.github.io/react-multivalue-text-input/styleguide/",
	"keywords": [
		"react",
		"reactjs",
		"text input",
		"input",
		"textinput",
		"ui",
		"user interface",
		"forms",
		"html"
	],
	"bugs": {
		"url": "https://github.com/blackjackkent/react-multivalue-text-input/issues",
		"email": "rosalind@blackjack-software.com"
	},
	"license": "MIT",
	"peerDependencies": {
		"react": "^16.3.2"
	},
	"dependencies": {
		"prop-types": "^15.6.1",
		"react-dom": "^16.3.2"
	},
	"scripts": {
		"pretest": "npm run lint",
		"test": "jest --coverage",
		"serve:docs": "npx styleguidist server",
		"build:docs": "npx styleguidist build",
		"build": "npm run clean && mkdir build && babel src -d build --copy-files",
		"clean": "rimraf ./build",
		"lint": "eslint src/",
		"lint:watch": "esw src/ -w",
		"np": "np --no-publish --branch=production"
	},
	"author": {
		"name": "Rosalind Wills",
		"email": "rosalind@blackjack-software.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blackjackkent/react-multivalue-text-input"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.8",
		"@babel/core": "^7.15.0",
		"@babel/preset-env": "^7.15.0",
		"@babel/preset-react": "^7.14.5",
		"@testing-library/dom": "^8.1.0",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^13.2.1",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^8.2.2",
		"css-hot-loader": "^1.3.9",
		"css-loader": "^0.28.11",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"eslint-watch": "^3.1.4",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^26.0.1",
		"node-sass": "^4.8.3",
		"react": "^16.3.2",
		"react-styleguidist": "^7.0.6",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"webpack": "^4.46.0"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>/utilities/styleMock.js"
		},
		"setupFilesAfterEnv": [
			"<rootDir>/utilities/testSetup.js"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/utilities/",
			"/build/components/"
		]
	}
}
