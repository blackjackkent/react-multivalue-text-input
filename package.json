{
	"name": "react-multivalue-text-input",
	"version": "0.6.0",
	"description": " A text input component for React which maintains and displays a collection of entered values as an array of strings.",
	"browser": "build/index.js",
	"main": "build/index.js",
	"homepage": "https://blackjackkent.github.io/react-multivalue-text-input/styleguide/",
	"keywords": [
		"react",
		"reactjs",
		"text input",
		"input",
		"textinput",
		"ui",
		"user interface",
		"forms",
		"html"
	],
	"bugs": {
		"url": "https://github.com/blackjackkent/react-multivalue-text-input/issues",
		"email": "rosalind@blackjack-software.com"
	},
	"license": "MIT",
	"peerDependencies": {
		"react": "^16.3.2"
	},
	"dependencies": {
		"prop-types": "^15.6.1",
		"react-dom": "^16.3.2"
	},
	"scripts": {
		"pretest": "yarn lint",
		"test": "jest --coverage",
		"serve:docs": "npx styleguidist server",
		"build:docs": "npx styleguidist build",
		"build": "yarn clean && mkdir build && babel src -d build --copy-files",
		"clean": "rimraf ./build",
		"lint": "eslint src/",
		"lint:watch": "esw src/ -w"
	},
	"author": {
		"name": "Rosalind Wills",
		"email": "rosalind@blackjack-software.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/blackjackkent/react-multivalue-text-input"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.3",
		"babel-loader": "^7.1.4",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-plugin-transform-runtime": "^6.23.0",
		"babel-preset-env": "^1.6.1",
		"css-hot-loader": "^1.3.9",
		"css-loader": "^0.28.11",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^16.1.0",
		"eslint-plugin-import": "^2.11.0",
		"eslint-plugin-jsx-a11y": "^6.0.3",
		"eslint-plugin-react": "^7.7.0",
		"eslint-watch": "^3.1.4",
		"extract-text-webpack-plugin": "^3.0.2",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^22.4.3",
		"jest-enzyme": "^6.0.0",
		"node-sass": "^4.8.3",
		"react": "^16.3.2",
		"react-styleguidist": "^7.0.6",
		"sass-loader": "^7.0.1",
		"style-loader": "^0.21.0",
		"webpack": "3.5.5"
	},
	"jest": {
		"moduleNameMapper": {
			"\\.(css|scss)$": "<rootDir>/utilities/styleMock.js"
		},
		"setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
		"testEnvironment": "enzyme",
		"testEnvironmentOptions": {
			"enzymeAdapter": "react16"
		},
		"coverageThreshold": {
			"global": {
				"branches": 100,
				"functions": 100,
				"lines": 100,
				"statements": 100
			}
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/utilities/",
			"/build/components/"
		]
	}
}
