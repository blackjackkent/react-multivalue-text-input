{
	"compilerOptions": {
		"declaration": true,
		"target": "esnext",
		"lib": ["esnext", "dom"],
		"strict": true,
		"noImplicitAny": false,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"outDir": "dist",
		"jsx": "react",
		"skipLibCheck": true
	},
	"include": ["src"],
	"exclude": [
		"node_modules",
		"**/*.test.ts",
		"**/*.test.tsx",
		"**/*.spec.tsx",
		"src/dev/dev.tsx",
		"src/build/build.tsx"
	]
}
