{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"declaration": true,
		"outDir": "./dist",
		"strict": true,
		"jsx": "react",
		"esModuleInterop": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"moduleResolution": "node",
		"skipLibCheck": true
	},
	"include": ["src"],
	"exclude": [
		"node_modules",
		"**/*.test.ts",
		"**/*.test.tsx",
		"**/*.spec.tsx",
		"src/dev/dev.tsx",
		"src/build/build.tsx"
	]
}
